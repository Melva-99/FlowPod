{
  "expo": {
    "name": "FlowPod",
    "slug": "FlowPod",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FlowPod.png",
    "splash": {
      "image": "./assets/fondo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/fondo.png"
    }
  }
}
